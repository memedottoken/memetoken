        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^11.0.1",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Envatic\\CrudStrap\\": "src/"
        }
