{
    "name": "envatic/crud-strap",
    "license": "MIT",
    "description": "Laravel CRUD Generator",
    "keywords": [
        "laravel",
        "crud",
        "crud generator",
        "laravel crud generator",
        "api generator"
    ],
    "authors": [
        {
            "name": "Steven Isaac",
            "email": "ofuzak@gmail.com"
        }
    ],
    "require": {
